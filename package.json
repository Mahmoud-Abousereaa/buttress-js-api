    "test-org": "mocha --require should --reporter nyan -g org",
    "test-person": "mocha --require should --reporter nyan -g person",
    "test-post": "mocha --require should --reporter nyan -g post",
    "test-task": "mocha --require should --reporter nyan -g task",
    "test-user": "mocha --require should --reporter nyan -g user"
